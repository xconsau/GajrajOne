	 sub Reph.dv Bindu.dv by RephBindu.dv;

	 sub Reph.dv Chandrabindu.dv by RephChandrabindu.dv;
	 sub matraEReph.dv Chandrabindu.dv by matraERephChandrabindu.dv;
	 sub matraAiReph.dv Chandrabindu.dv by matraAiRephChandrabindu.dv;
	 sub matraShortEReph.dv Chandrabindu.dv by matraShortERephChandrabindu.dv;
} psts3;


lookup psts2 {
	 sub matraIi.dv RephChandrabindu.dv by matraIiRephChandrabindu.dv;
	 sub matraO.dv RephChandrabindu.dv by matraORephChandrabindu.dv;
	 sub matraAu.dv RephChandrabindu.dv by matraAuRephChandrabindu.dv;
	 sub matraA.dv RephChandrabindu.dv by matraARephChandrabindu.dv;
	 sub matraShortO.dv RephChandrabindu.dv by matraShortORephChandrabindu.dv;

	 sub matraIi.dv Chandrabindu.dv by matraIiChandrabindu.dv;
	 sub matraO.dv Chandrabindu.dv by matraOChandrabindu.dv;
	 sub matraAu.dv Chandrabindu.dv by matraAuChandrabindu.dv;
	 sub matraA.dv Chandrabindu.dv by matraAChandrabindu.dv;
	 sub matraShortO.dv Chandrabindu.dv by matraShortOChandrabindu.dv;

	 sub matraIi.dv Bindu.dv by matraIiBindu.dv;
	 sub matraShortO.dv Bindu.dv by matraShortOBindu.dv;
	 sub matraO.dv Bindu.dv by matraOBindu.dv;
	 sub matraAu.dv Bindu.dv by matraAuBindu.dv;

	 sub matraIi.dv RephBindu.dv by matraIiRephBindu.dv;
	 sub matraO.dv RephBindu.dv by matraORephBindu.dv;
	 sub matraShortE.dv RephBindu.dv by matraShortERephBindu.dv;
	 sub matraShortO.dv RephBindu.dv by matraShortORephBindu.dv;
	 sub matraAu.dv RephBindu.dv by matraAuRephBindu.dv;

	 sub matraIi.dv Reph.dv by matraIiReph.dv;
	 sub matraShortO.dv Reph.dv by matraShortOReph.dv;
	 sub matraO.dv Reph.dv by matraOReph.dv;
	 sub matraAu.dv Reph.dv by matraAuReph.dv;
} psts2;


lookup psts1 { # Postbase matra substitutions
	sub @Width_matraIione @matraIidefault' by @matraIione;
	sub @Width_matraIitwo @matraIidefault' by @matraIitwo;
	sub @Width_matraIithree @matraIidefault' by @matraIithree;
} psts1;

lookup psts6 { # Postbase matra substitutions with vattu
	sub @Width_matraIione Vattu.dv @matraIidefault' by @matraIione;
	sub @Width_matraIitwo Vattu.dv @matraIidefault' by @matraIitwo;
	sub @Width_matraIithree Vattu.dv @matraIidefault' by @matraIithree;
} psts6;



} psts;


