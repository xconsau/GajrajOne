feature pres { # Pre-base Substitutions


	lookup pres4 { # Four-letter conjuncts

	} pres4;

	lookup pres3 { # Three-letter conjuncts
		sub Cha.dv Halant.dv Ra.dv Halant.dv Ya.dv by ChaRaYa.dv;
		sub Tta.dv Halant.dv Tta.dv Halant.dv Ya.dv by TtaTtaYa.dv;
		sub Ttha.dv Halant.dv Ttha.dv Halant.dv Ya.dv by TthaTthaYa.dv;
		sub Dda.dv Halant.dv Dda.dv Halant.dv Ya.dv by DdaDdaYa.dv;
		sub Ddha.dv Halant.dv Ra.dv Halant.dv Ya.dv by DdhaRaYa.dv;
		sub Da.dv Halant.dv Da.dv Halant.dv Ya.dv by DaDaYa.dv;
		sub DaDa.dv Halant.dv Ya.dv by DaDaYa.dv;
		sub Da.dv Halant.dv DhaHalant.dv Ya.dv by DaDhaYa.dv;
		sub Da.dv Halant.dv BhaHalant.dv Ya.dv by DaBhaYa.dv;
		sub DhaHalant.dv NaHalant.dv Ya.dv by DhaNaYa.dv;
		sub ShaRaHalant.dv Ya.dv by ShaRaYa.dv;
		sub TaHalant.dv NaHalant.dv Ya.dv by TaNaYa.dv;
		sub GaHalant.dv NaHalant.dv Ya.dv by GaNaYa.dv;

	} pres3;
		
	lookup pres2 { # Two-letter conjuncts
		sub KaHalant.dv Ta.dv by KaTa.dv;
		sub TaHalant.dv Ta.dv by TaTa.dv;
		sub NaHalant.dv Na.dv by NaNa.dv;
		sub Cha.dv Halant.dv Ya.dv by ChaYa.dv;
		sub Cha.dv Halant.dv Va.dv by ChaVa.dv;
		sub Tta.dv Halant.dv Tta.dv by TtaTta.dv;
		sub Tta.dv Halant.dv Ttha.dv by TtaTtha.dv;
		sub Tta.dv Halant.dv Ya.dv by TtaYa.dv;
		sub Tta.dv Halant.dv Va.dv by TtaVa.dv;
		sub Ttha.dv Halant.dv Ya.dv by TthaYa.dv;
		sub Ttha.dv Halant.dv Ttha.dv by TthaTtha.dv;
		sub Dda.dv Halant.dv Dda.dv by DdaDda.dv;
		sub Dda.dv Halant.dv Ddha.dv by DdaDdha.dv;
		sub Dda.dv Halant.dv Ya.dv by DdaYa.dv;
		sub Ddha.dv Halant.dv Ddha.dv by DdhaDdha.dv;
		sub Ddha.dv Halant.dv Ya.dv by DdhaYa.dv;
		sub Da.dv Halant.dv Ga.dv by DaGa.dv;
		sub MaHalant.dv Na.dv by MaNa.dv;
		sub LlaHalant.dv Ya.dv by LlaYa.dv;
		sub LlaHalant.dv Lla.dv by LlaLla.dv;
		sub ShaHalant.dv Na.dv by ShaNa.dv;
		sub SaHalant.dv Na.dv by SaNa.dv;
		sub Ha.dv Halant.dv Nna.dv by HaNna.dv;
		sub Ha.dv Halant.dv Ma.dv by HaMa.dv;
		sub Ha.dv Halant.dv Ya.dv by HaYa.dv;
		sub Ha.dv Halant.dv Va.dv by HaVa.dv;
		sub BaHalant.dv Na.dv by BaNa.dv;
		sub PaHalant.dv Na.dv by PaNa.dv;
		sub DhaHalant.dv Na.dv by DhaNa.dv;
		sub TaHalant.dv Na.dv by TaNa.dv;
		sub JhaHalant.dv Na.dv by JhaNa.dv;
		sub CaHalant.dv Na.dv by CaNa.dv;
		sub GhaHalant.dv Na.dv by GhaNa.dv;
		sub GaHalant.dv Na.dv by GaNa.dv;
		sub KhaHalant.dv Na.dv by KhaNa.dv;
		sub Da.dv Halant.dv Va.dv by DaVa.dv;
		sub Da.dv Halant.dv Bha.dv by DaBha.dv;
		sub Da.dv Halant.dv Na.dv by DaNa.dv;
		sub SsaHalant.dv Tta.dv by SsaTta.dv;
		sub ShaHalant.dv Va.dv by ShaVa.dv;
		sub SsaHalant.dv Ttha.dv by SsaTtha.dv;
		sub JaHalant.dv Na.dv by JaNa.dv;

	} pres2;


	lookup pres_half { # half-conjuncts
		sub TaHalant.dv TaHalant.dv by TaTaHalant.dv;

	} pres_half;

	

	##-------------------------------------------
	# Matra substitution

	# Bridge width = Glyph group width - 180
	#--------------------------------------------

	lookup pres_matraI { # matraI substitutions
		sub matraI.dv' @width_340 by matraI_left.dv bridge_100.dv bridge_60.dv matraI_right.dv;
		sub matraI.dv' @width_550 by matraI_left.dv bridge_300.dv bridge_70.dv matraI_right.dv;
		sub matraI.dv' @width_600 by matraI_left.dv bridge_400.dv bridge_20.dv matraI_right.dv;
		sub matraI.dv' @width_640 by matraI_left.dv bridge_400.dv bridge_60.dv matraI_right.dv;
		sub matraI.dv' @width_720 by matraI_left.dv bridge_500.dv bridge_40.dv matraI_right.dv;
		sub matraI.dv' @halant_width_420 @width_340 by matraI_left.dv bridge_500.dv bridge_80.dv matraI_right.dv;
		sub matraI.dv' @width_800 by matraI_left.dv bridge_600.dv bridge_20.dv matraI_right.dv;
		sub matraI.dv' @halant_width_480 @width_340 by matraI_left.dv bridge_600.dv bridge_40.dv matraI_right.dv;
		sub matraI.dv' @width_840 by matraI_left.dv bridge_600.dv bridge_60.dv matraI_right.dv;
		sub matraI.dv' @halant_width_510 @width_340 by matraI_left.dv bridge_600.dv bridge_70.dv matraI_right.dv;
		sub matraI.dv' @halant_width_590 @width_340 by matraI_left.dv bridge_700.dv bridge_50.dv matraI_right.dv;
		sub matraI.dv' @width_950 by matraI_left.dv bridge_700.dv bridge_70.dv matraI_right.dv;
		sub matraI.dv' @halant_width_420 @width_550 by matraI_left.dv bridge_700.dv bridge_90.dv matraI_right.dv;
		sub matraI.dv' @halant_width_420 @width_600 by matraI_left.dv bridge_800.dv bridge_40.dv matraI_right.dv;
		sub matraI.dv' @halant_width_680 @width_340 by matraI_left.dv bridge_800.dv bridge_40.dv matraI_right.dv;
		sub matraI.dv' @halant_width_480 @width_550 by matraI_left.dv bridge_800.dv bridge_50.dv matraI_right.dv;
		sub matraI.dv' @halant_width_710 @width_340 by matraI_left.dv bridge_800.dv bridge_70.dv matraI_right.dv;
		sub matraI.dv' @halant_width_420 @width_640 by matraI_left.dv bridge_800.dv bridge_80.dv matraI_right.dv;
		sub matraI.dv' @halant_width_510 @width_550 by matraI_left.dv bridge_800.dv bridge_80.dv matraI_right.dv;
		sub matraI.dv' @halant_width_730 @width_340 by matraI_left.dv bridge_800.dv bridge_90.dv matraI_right.dv;
