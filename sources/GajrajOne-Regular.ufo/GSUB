#--------------------------------------------------------------------

feature cjct {
	sub Da.dv Halant.dv Da.dv by DaDa.dv;
	sub Da.dv Halant.dv Ma.dv by DaMa.dv;
	sub Da.dv Halant.dv Ya.dv by DaYa.dv;
	sub DaRaHalant.dv Ya.dv by DaRaYa.dv;
	sub Da.dv Halant.dv VaHalant.dv Ya.dv by DaVaYa.dv;
	sub Da.dv Halant.dv Va.dv Halant.dv Ya.dv by DaVaYa.dv;
	sub Nga.dv Halant.dv Ka.dv by NgaKa.dv;
	sub Nga.dv Halant.dv Ga.dv by NgaGa.dv;
	sub Nga.dv Halant.dv Ma.dv by NgaMa.dv;
	sub Nga.dv Halant.dv Ya.dv by NgaYa.dv;
	sub Ha.dv Halant.dv MaHalant.dv Ya.dv by HaMaYa.dv;
	sub Da.dv Halant.dv Ba.dv by DaBa.dv;
	sub Da.dv Halant.dv Dha.dv by DaDha.dv;
} cjct;



#--------------------------------------------------------------------

# The next four lookup operations are to get the right output for Ra.dv + matraRu.dv = vowelRu.dv Reph.dv, etc. 

lookup vowelRuReph {
     sub Ra.dv by vowelRu.dv;
     sub matraRu.dv by Reph.dv;
 } vowelRuReph;

lookup vowelRuuReph {
     sub Ra.dv by vowelRuu.dv;
     sub matraRuu.dv by Reph.dv;
 } vowelRuuReph;

 lookup vowelLruReph {
     sub Ra.dv by vowelLru.dv;
     sub matraLru.dv by Reph.dv;
 } vowelLruReph;

lookup vowelLruuReph {
     sub Ra.dv by vowelLruu.dv;
     sub matraLruu.dv by Reph.dv;
 } vowelLruuReph;

#------------------------------------


feature abvs { # Abovebase matra substitutions

lookup abvs8 {
	sub matraI_right.dv' @all_width Bindu.dv by matraIBindu_right.dv;
	sub matraI_right.dv' @all_halant @all_width Bindu.dv by matraIBindu_right.dv;
}abvs8;

lookup abvs8dummy {
	sub matraIBindu_right.dv @all_width Bindu.dv' by dummy1;
	sub matraIBindu_right.dv @all_halant @all_width Bindu.dv' by dummy1;
} abvs8dummy;

lookup abvs7{
	sub Ra.dv' lookup vowelRuReph matraRu.dv' lookup vowelRuReph;
	sub Ra.dv' lookup vowelRuuReph matraRuu.dv' lookup vowelRuuReph;
	sub Ra.dv' lookup vowelLruReph matraLru.dv' lookup vowelLruReph;
	sub Ra.dv' lookup vowelLruuReph matraLruu.dv' lookup vowelLruuReph;		
}abvs7;

lookup abvs6 {

} abvs6;


lookup abvs5 {
	sub vowelAi.dv Chandrabindu.dv' by Chandrabinduone.dv;
	sub vowelO.dv Chandrabindu.dv' by Chandrabinduone.dv;
	sub vowelAu.dv Chandrabindu.dv' by Chandrabinduone.dv;
	sub vowelShortA.dv Chandrabindu.dv' by Chandrabinduone.dv;
	sub vowelShortE.dv Chandrabindu.dv' by Chandrabinduone.dv;
	sub vowelShortO.dv Chandrabindu.dv' by Chandrabinduone.dv;
} abvs5;


lookup abvs4 {
	sub matraI_right.dv @all_width Vattu.dv Bindu.dv' by Binduone.dv;
} abvs4;


lookup abvs3 {
	sub vowelAi.dv Bindu.dv' by Binduone.dv;
	sub vowelO.dv Bindu.dv' by Binduone.dv;
	sub vowelAu.dv Bindu.dv' by Binduone.dv;
	sub vowelShortA.dv Bindu.dv' by Binduone.dv;
	sub vowelShortE.dv Bindu.dv' by Binduone.dv;
	sub vowelShortO.dv Bindu.dv' by Binduone.dv;
} abvs3;

lookup abvs2 { 
	sub matraI_right.dv' @all_width Reph.dv by matraIReph_right.dv;
	sub matraI_right.dv' @all_halant @all_width Reph.dv by matraIReph_right.dv;
} abvs2;

