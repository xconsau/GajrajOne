feature half  {  # Half Forms
	script dev2; 
	language dflt;
	lookup half_rakar {
		 sub KaRa.dv Halant.dv by KaRaHalant.dv;
		 sub GaRa.dv Halant.dv by GaRaHalant.dv;
		 sub GhaRa.dv Halant.dv by GhaRaHalant.dv;
		 sub TaRa.dv Halant.dv by TaRaHalant.dv;
		 sub DaRa.dv Halant.dv by DaRaHalant.dv;
