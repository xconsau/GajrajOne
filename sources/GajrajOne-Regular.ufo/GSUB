	sub matraIRephChandrabindu_right.dv @all_halant @all_width dummy1 Chandrabindu.dv' by dummy1;
	sub matraIChandrabindu_right.dv @all_width Chandrabindu.dv' by dummy1;
	sub matraIChandrabindu_right.dv @all_halant @all_width Chandrabindu.dv' by dummy1;
} abvs2dummychandrabindu;

lookup abvs2vattu { 
 #sub matraI.dv' @width_600 Vattu.dv Reph.dv by matraIReph.dv;
} abvs2vattu;



