feature blws { # Belowbase substitution

lookup blws1 {
	sub Ra.dv matraU.dv by RamatraU.dv;
	sub Ra.dv matraUu.dv by RamatraUu.dv;
	sub Ha.dv matraRu.dv by HamatraRu.dv;
	sub Sha.dv matraRu.dv by ShamatraRu.dv;
	sub Ka.dv matraLru.dv by KamatraLru.dv;
	sub Ka.dv matraLruu.dv by KamatraLruu.dv;
	sub Pa.dv matraLru.dv by PamatraLru.dv;
	sub Pa.dv matraLruu.dv by PamatraLruu.dv;
} blws1;


lookup blws_Vattumatra {
	sub Vattu.dv [matraU.dv] by VattumatraU.dv;
	sub Vattu.dv [matraUu.dv] by VattumatraUu.dv;
} blws_Vattumatra;


lookup blws_Vertical_Conjuncts {
	sub @Vertical_Conjuncts matraU.dv' by matraUsmall.dv;
	sub @Vertical_Conjuncts matraUu.dv' by matraUusmall.dv;
	sub @Vertical_Conjuncts matraRu.dv' by matraRusmall.dv;
} blws_Vertical_Conjuncts;

} blws;



