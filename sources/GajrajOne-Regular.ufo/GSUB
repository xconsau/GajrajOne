	sub matraI_right.dv' @all_width Chandrabindu.dv by matraIChandrabindu_right.dv;
	sub matraI_right.dv' @all_halant @all_width Chandrabindu.dv by matraIChandrabindu_right.dv;
} abvs2chandrabindu;

lookup abvs2dummybindu {
